library fuchsia.hardware.registers;

using zx;

@transport("Banjo")
@banjo_layout("ddk-protocol")
protocol Registers {
    /// Gets a channel for FIDL protocol.
    /// |chan| : Channel for FIDL protocol.
    Connect(resource struct {
        chan zx.handle:CHANNEL;
    }) -> ();
};
