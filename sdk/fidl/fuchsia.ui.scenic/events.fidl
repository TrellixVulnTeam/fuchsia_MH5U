library fuchsia.ui.scenic;

using fuchsia.ui.input;
using fuchsia.ui.gfx;

type Event = strict resource union {
    1: gfx fuchsia.ui.gfx.Event;
    2: input fuchsia.ui.input.InputEvent;
    3: unhandled Command;
};
