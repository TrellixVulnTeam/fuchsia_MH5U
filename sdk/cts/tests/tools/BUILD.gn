group("tests_no_e2e") {
  testonly = true
  assert_no_deps = e2e_test_libs
  deps = []
}

group("e2e_tests") {
  testonly = true
  deps = [ "package_manager:tests" ]
}

group("tests") {
  testonly = true
  deps = [
    ":e2e_tests",
    ":tests_no_e2e",
  ]
}
