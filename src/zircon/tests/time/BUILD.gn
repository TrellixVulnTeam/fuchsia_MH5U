import("//build/test.gni")
import("//build/testing/bootfs_test.gni")

test("time") {
  output_dir = "$root_out_dir/zircon-migrated"
  sources = [ "time_test.cc" ]
  deps = [
    "//sdk/lib/fdio",
    "//zircon/system/ulib/zxtest",
  ]
}

bootfs_test("time-bootfs-test") {
  name = "time"
  deps = [ ":time" ]
}
