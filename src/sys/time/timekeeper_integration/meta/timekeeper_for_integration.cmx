{
    "include": [
        "syslog/client.shard.cmx"
    ],
    "program": {
        "args": [
            "--dev-time-sources",
            "--disable-delays"
        ],
        "binary": "bin/timekeeper"
    },
    "sandbox": {
        "services": [
            "fuchsia.cobalt.LoggerFactory",
            "fuchsia.net.interfaces.State",
            "fuchsia.posix.socket.Provider",
            "fuchsia.sys.Launcher",
            "fuchsia.time.Maintenance"
        ]
    }
}
