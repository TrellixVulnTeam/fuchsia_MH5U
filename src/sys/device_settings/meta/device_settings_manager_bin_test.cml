{
    include: [ "syslog/client.shard.cml" ],
    program: {
        runner: "rust_test_runner",
        binary: "bin/device_settings_manager_bin_test",
    },
    capabilities: [
        { protocol: "fuchsia.test.Suite" },
    ],
    use: [
        {
            storage: "tmp",
            path: "/tmp",
        },
    ],
    expose: [
        {
            protocol: "fuchsia.test.Suite",
            from: "self",
        },
    ],
}
