{
    include: [ "syslog/client.shard.cml" ],
    program: {
        runner: "elf",
        binary: "bin/network_tun",
    },
    capabilities: [
        { protocol: "fuchsia.net.tun.Control" },
    ],
    expose: [
        {
            protocol: "fuchsia.net.tun.Control",
            from: "self",
        },
    ],
}
