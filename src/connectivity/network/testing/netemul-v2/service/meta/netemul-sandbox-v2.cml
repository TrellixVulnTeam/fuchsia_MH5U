{
    include: [ "//src/connectivity/network/testing/netemul-v2/service/meta/common.shard.cml" ],
    program: {
        runner: "elf",
        binary: "bin/netemul_sandbox_v2",
    },
    capabilities: [
        { protocol: "fuchsia.netemul.Sandbox" },
    ],
    expose: [
        {
            protocol: "fuchsia.netemul.Sandbox",
            from: "self",
        },
        {
            protocol: "fuchsia.net.tun.Control",
            from: "#network-tun",
        },
    ],
}
