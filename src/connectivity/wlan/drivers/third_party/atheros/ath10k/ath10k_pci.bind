using fuchsia.pci;

fuchsia.BIND_PROTOCOL == fuchsia.pci.BIND_PROTOCOL.DEVICE;

fuchsia.BIND_PCI_VID == fuchsia.pci.BIND_PCI_VID.ATHEROS;

accept fuchsia.BIND_PCI_DID {
  0x003c, // QCA988X_2_0
  0x003e, // QCA6174_2_1
  0x0040, // QCA99X0_2_0
  0x0041, // QCA6164_2_1
  0x0042, // QCA9377_1_0
  0x0046, // QCA9984_1_0
  0x0050, // QCA9887_1_0
  0x0056, // QCA9888_2_0
}
fuchsia.BIND_COMPOSITE == 1;
