{
    "facets": {
        "fuchsia.test": {
            "system-services": [
                "fuchsia.kernel.RootJob"
            ]
        }
    },
    "include": [
        "syslog/client.shard.cmx"
    ],
    "program": {
        "args": [
            "/pkg/bin/process_builder_lib_test",
            "-Zunstable-options",
            "--force-run-in-process"
        ],
        "binary": "bin/root_job_test_runner"
    },
    "sandbox": {
        "services": [
            "fuchsia.kernel.RootJob",
            "fuchsia.process.Launcher"
        ]
    }
}
