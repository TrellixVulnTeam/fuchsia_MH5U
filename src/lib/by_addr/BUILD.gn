import("//build/components.gni")
import("//build/rust/rustc_library.gni")

rustc_library("by_addr") {
  version = "0.1.0"
  edition = "2018"
  with_unit_tests = true

  sources = [ "src/lib.rs" ]
}

fuchsia_unittest_package("by_addr_tests") {
  deps = [ ":by_addr_test" ]
}

group("tests") {
  testonly = true
  deps = [ ":by_addr_tests" ]
}
