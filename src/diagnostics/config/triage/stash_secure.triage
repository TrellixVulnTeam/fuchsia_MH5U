{
    select: {
        status: [
            "INSPECT:core/stash_secure:root/fuchsia.inspect.Health:status",
            "INSPECT:stash_secure.cmx:root/fuchsia.inspect.Health:status",
        ],
        is_secure: [
            "INSPECT:core/stash_secure:root:secure_mode",
            "INSPECT:stash_secure.cmx:root:secure_mode",
        ],
    },
    act: {
        unhealthy: {
            type: "Warning",
            trigger: "status != 'OK'",
            print: "Stash server is experiencing errors",
        },
        should_be_secure: {
            type: "Warning",
            trigger: "Not(is_secure)",
            print: "The secure stash should be running in secure mode",
        },
    },
    test: {
        failure: {
            yes: [
                "unhealthy",
                "should_be_secure",
            ],
            no: [],
            values: {
                status: "UNHEALTHY",
                is_secure: false,
            },
        },
        ok: {
            yes: [],
            no: [
                "unhealthy",
                "should_be_secure",
            ],
            values: {
                status: "OK",
                is_secure: true,
            },
        },
    },
}
