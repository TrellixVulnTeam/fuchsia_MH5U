library fuchsia.samplertestcontroller;

type SamplingError = strict enum : int32 {
    MULTIPLE_SAMPLE_CALLBACKS_ERROR = 1;
};

@discoverable
protocol SamplerTestController {
    IncrementInt(struct {
        property_id uint16;
    });
    WaitForSample() -> (struct {}) error SamplingError;
};
