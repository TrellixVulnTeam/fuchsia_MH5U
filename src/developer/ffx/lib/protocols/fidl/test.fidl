library fuchsia.ffx.test;

@discoverable
protocol Noop {
    /// Does a noop.
    DoNoop() -> ();
};

@discoverable
protocol Counter {
    /// Adds one to the internal state.
    AddOne() -> ();

    /// Gets the internal state.
    GetCount() -> (struct {
        count uint64;
    });
};
