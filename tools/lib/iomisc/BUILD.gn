import("//build/go/go_library.gni")
import("//build/go/go_test.gni")

go_library("iomisc") {
  sources = [
    "doc.go",
    "iomisc.go",
    "match.go",
    "match_test.go",
    "reader.go",
    "reader_test.go",
  ]
  deps = [ "//tools/lib/logger" ]
}

go_test("tests") {
  output_name = "iomisc_tests"
  gopackages = [ "go.fuchsia.dev/fuchsia/tools/lib/iomisc" ]
  deps = [ ":iomisc" ]
}
