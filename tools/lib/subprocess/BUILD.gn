import("//build/go/go_library.gni")
import("//build/go/go_test.gni")

go_library("subprocess") {
  sources = [
    "doc.go",
    "subprocess.go",
    "subprocess_test.go",
  ]
  deps = [
    "//tools/lib/clock",
    "//tools/lib/logger",
