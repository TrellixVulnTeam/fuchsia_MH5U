{{~ #if (eq element_type.kind "identifier") ~}}
    vector&lt;{{~ pl element_type.identifier @root.name ~}}&gt;
{{~ else ~}}
    {{~ #if (eq element_type.kind "vector") ~}}
        {{~ #with element_type ~}}
            vector&lt;{{> vector }}&gt;
        {{~ /with ~}}
    {{~ else ~}}
        {{~ #if element_type.subtype ~}}
            vector&lt;{{~ element_type.subtype ~}}&gt;
        {{~ else ~}}
            vector&lt;{{~ element_type.kind ~}}&gt;
        {{~ /if ~}}
    {{~ /if ~}}
{{~ /if ~}}