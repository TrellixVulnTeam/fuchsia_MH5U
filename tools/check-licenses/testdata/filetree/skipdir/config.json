{
   "skipFiles":[
      ".gitignore"
   ],
   "skipDirs":[
      "{root}/skipMe",
      "{root}/skipMeCompletely",
      "{root}/dontSkip/skipMe"
   ],
   "dontSkipDirs":[
      "{root}/skipMe/dontSkipMe"
   ],
   "textExtensionList":[
      "go",
      "py"
   ],
   "maxReadSize":6144,
   "outputFilePrefix":"NOTICE",
   "singleLicenseFiles":[
      "LICENSE"
   ],
   "licensePatternDir":"golden/",
   "baseDir":".",
   "target":"all",
   "logLevel":"verbose",
   "customProjectLicenses":[
      {
         "projectRoot":"test",
         "licenseLocation":"test"
      }
   ]
}
