{
   "skipFiles":[
      ".gitignore",
      ".empty"
   ],
   "skipDirs":[
      ".git"
   ],
   "textExtensionList":[
      "go",
      "py"
   ],
   "maxReadSize":6144,
   "outputFilePrefix":"NOTICE",
   "outputFileExtensions":[
       "txt"
   ],
   "singleLicenseFiles":[
      "LICENSE"
   ],
   "licensePatternDir":"golden/",
   "baseDir":".",
   "target":"all",
   "logLevel":"verbose",
   "customProjectLicenses":[
      {
         "projectRoot":"test",
         "licenseLocation":"test"
      }
   ]
}
