{
    "include": [
        "syslog/client.shard.cmx"
    ],
    "program": {
        "binary": "bin/insntrace"
    },
    "sandbox": {
        "dev": [
            "sys/cpu-trace"
        ],
        "features": [
            "isolated-persistent-storage",
            "isolated-temp"
        ],
        "services": [
            "fuchsia.tracing.kernel.Controller",
            "fuchsia.tracing.kernel.Reader"
        ]
    }
}
