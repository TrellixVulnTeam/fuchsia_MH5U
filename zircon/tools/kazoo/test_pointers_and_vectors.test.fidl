library zx;

@transport("Syscall")
protocol TypeMappings {
    Func0(struct {
        bytes vector<byte>;
        str string;
    }) -> (struct {
        status status;
    });
    Func1(resource struct {
        ins vector<handle>;
    }) -> (struct {
        status status;
    });
    Func2();
};
