library zx;

@transport("Syscall")
protocol Couple {
    /// This does a single thing.
    @internal
    DoThing(struct {
        an_input int32;
    }) -> (struct {
        status status;
    });

    /// Does great stuff.
    GetStuff(struct {
        an_input int32;
        input2 int32;
    }) -> (struct {
        status status;
    });
};
