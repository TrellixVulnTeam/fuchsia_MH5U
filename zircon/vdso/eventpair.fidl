library zx;

@transport("Syscall")
protocol eventpair {
    /// Create an event pair.
    eventpair_create(struct {
        options uint32;
    }) -> (resource struct {
        status status;
        out0 handle:EVENT;
        out1 handle:EVENT;
    });
};
